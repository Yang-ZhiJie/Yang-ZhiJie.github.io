/* pages/index3/index3.wxss */
.nav {
  width: 100%;
  /* overflow: hidden; */
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background:#3AB44A;
}
.title_text {
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-size: 20px;
  color: #fff;
}
.title_icon {
  position: absolute;
  bottom: 10px;
  left: 20px;
  box-sizing: border-box;
  z-index: 20;
  
}
.title_icon>text{
  font-size: 35rpx;
  color: #fff;
  font-weight: bolder;
}